<!DOCTYPE html>
<html data-ng-app="xstore" data-ng-nicescroll="" xmlns:ng="http://angularjs.org" id="ng-app" data-ng-controller="RootController" @*data-xstore-nice-scroll=""*@ data-auto-hide="false">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width" />
    <title ng-bind="title"></title>
    @Styles.Render("~/Content/Plugins/css")
    @Styles.Render("~/Content/css")
    @Styles.Render("~/Content/select")
    @Styles.Render("~/Content/carsoule")
    @Scripts.Render("~/bundles/modernizr")
</head>
<body>
    <div id="main-wrapper">

        @Html.Partial("_TopNav")
        @*@{Html.RenderAction("LeftNav", "Home");}*@

        <div class="snaker animated fadeIn high-index loading">
            <div class="circle1"></div>
            <div class="circle2"></div>
            <div class="circle3"></div>
            <div class="circle4"></div>
            <div class="circle5"></div>
            <div class="circle6"></div>
            <h6>LOADING</h6>
        </div>

        <div id="page-wrapper" class="container-fluid clear-side-padding" resize ng-style="resizeWithOffset(65)" data-ng-class="{'slide-leftNav':appSettings.isLeftNavPushed}">

            <div class="clearfix page-container">
                <div class="notification" data-xstore-post-data-notification="">
                    <p>{{notificationMessage}}</p>
                </div>

                @RenderBody()
            </div>
        </div>

        @Scripts.Render("~/bundles/jquery")
        @Scripts.Render("~/bundles/bootstrap")
        @Scripts.Render("~/bundles/angular")
        @Scripts.Render("~/bundles/d3")
        @Scripts.Render("~/bundles/libraries")
        @RenderSection("scripts", required: false)
    </div>
</body>
</html>